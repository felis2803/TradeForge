{
  "name": "@tradeforge/sdk",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm --out-dir dist && node -e \"require('fs').cpSync('src/dashboard.html', 'dist/dashboard.html')\"",
    "clean": "rimraf dist",
    "format": "prettier -w .",
    "lint": "eslint . --ext .ts",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --config ../../jest.config.mjs --testPathPattern packages/sdk/tests",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "@tradeforge/core": "workspace:*",
    "@tradeforge/io-binance": "workspace:*",
    "@tradeforge/sim": "workspace:*",
    "puppeteer-core": "^24.31.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/ws": "^8.5.10",
    "tsup": "^8.0.0"
  }
}
